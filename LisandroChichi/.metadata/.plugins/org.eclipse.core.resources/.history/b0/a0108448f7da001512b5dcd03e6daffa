package BuilderConnectionDatabase;

public class MainConnection {

	public static void main(String[] args) {
				
		
		BuilderConnection builder = new OracleConnection();
		DirectorConnection director = new DirectorConnection(builder);
		
		
		director.constructConnection();
		
		
		Connection conn = new Connection();
		
		
		System.out.println("Connection Driver: "+conn.getDriver() 
				+"\nConnection Password: "+conn.getPass());
		
	}
	
}
